{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es2017", "dom"],
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowJs": true,
    "resolveJsonModule": true,
    "types": ["cesium", "node"]
  },
  "include": ["src/**/*"]
}